        function autoScroll(obj){
            $(obj).find("ul").animate({
                marginTop : "-"+$high+"px"
            },500,function(){
                $(this).css({marginTop : "0px"}).find("li:first").appendTo(this);
            })
        }
        $(function(){
            setInterval('autoScroll(".hl-player-notice")',3500);
